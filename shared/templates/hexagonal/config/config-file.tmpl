app:
  name: rocket-app
  port: 7000
  debug:
    config: false
  fiber:
    enable_print_routes: true
    enable_splitting_on_parsers: true
  {{- if .IsRedis}}
  cache:
    redis:
      addr: localhost:6379
      username:
      password:
      db: 0
  {{- end}}
  {{- if .IsPSQL}}
  databases:
    psql:
      host: localhost
      port: 5432
      user: postgres
      password: postgres
      database: postgres
  {{- end}}
